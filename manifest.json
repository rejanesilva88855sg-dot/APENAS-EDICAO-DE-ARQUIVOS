{
  "manifest_version": 3,
  "name": "Preenchedor de Cartões",
  "version": "1.0",
  "description": "Preenche formulários de cartão de crédito automaticamente.",
  "permissions": ["storage", "scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Cartões Salvos"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
